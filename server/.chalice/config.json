{
  "version": "2.0",
  "app_name": "data-dashboard-v4",
  "api_gateway_endpoint_type": "REGIONAL",
  "minimum_compression_size": 1000,
  "lambda_timeout": 30,
  "lambda_memory_size": 512,
  "stages": {
    "production": {
      "api_gateway_stage": "production",
      "autogen_policy": false,
      "iam_policy_file": "policy.json",
      "environment_variables": {
        "TM_FRONTEND_HOST": "dashboard-v4.transitmatters.org"
      },
      "api_gateway_custom_domain": {
        "domain_name": "dashboard-v4-api.transitmatters.org",
        "tls_version": "TLS_1_2",
        "certificate_arn": ""
      }
    },
    "beta": {
      "api_gateway_stage": "beta",
      "autogen_policy": false,
      "iam_policy_file": "policy.json",
      "environment_variables": {
        "TM_FRONTEND_HOST": "dashboard-v4-beta.labs.transitmatters.org"
      },
      "api_gateway_custom_domain": {
        "domain_name": "dashboard-v4-beta-api.labs.transitmatters.org",
        "tls_version": "TLS_1_2",
        "certificate_arn": ""
      }
    }
  }
}
