{
  "name": "t-dash",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@tippyjs/react": "^4.0.2",
    "chart.js": "^2.9.4",
    "classnames": "^2.2.6",
    "concurrently": "^5.2.0",
    "flatpickr": "4.5.7",
    "react": "^16.13.1",
    "react-chartjs-2": "^2.11.1",
    "react-dom": "^16.13.1",
    "react-ga": "^3.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "react-select": "^3.1.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0"
  },
  "homepage": ".",
  "scripts": {
    "start": "concurrently npm:start-python npm:start-react",
    "start-python": "cd server && pipenv run chalice local --port=5000",
    "start-react": "react-scripts start",
    "build": "react-scripts build",
    "lint": "npm run lint-frontend && npm run lint-backend",
    "lint-frontend": "eslint src",
    "lint-backend": "cd server && pipenv run flake8 --ignore=E501,E731 --exclude .chalice",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "cd server && pipenv install --python=3.7"
  },
  "proxy": "http://localhost:5000",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
